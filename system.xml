<?xml version="1.0" encoding="UTF-8"?>
<programs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="games.xsd" majorVersion="2" minorVersion="0" date="2012-06-18T20:32:21">
  <!--Contains profiles for backing up system-related files-->
  <system name="GamesForWindows">
    <title>Games for Windows</title>
    <version os="Windows">
      <locations>
        <path environment_variable="localappdata" path="Microsoft\XLive\Content"/>
      </locations>
      <files type="AccountData">
        <save/>
      </files>
      <contributor>SanMadJack</contributor>
    </version>
  </system>
  <system name="MASGAUCustomXML">
    <title>MASGAU Custom XML</title>
    <version>
      <locations>
        <path environment_variable="installlocation" path="MASGAU"/>
        <registry root="local_machine" key="SOFTWARE\MASGAU" value="InstallPath"/>
      </locations>
      <files type="Saves">
        <save filename="custom.xml"/>
      </files>
      <identifier filename="custom.xml"/>
      <contributor>SanMadJack</contributor>
    </version>
  </system>
  <system name="Raptr">
    <title>Raptr</title>
    <version os="Windows">
      <locations>
        <path environment_variable="appdata" path="Raptr"/>
      </locations>
      <files type="Data">
        <save path="data"/>
        <save path="Unknown"/>
        <save filename="raptr*">
          <except filename="*.exe"/>
        </save>
      </files>
      <contributor>Arc Angel</contributor>
    </version>
  </system>
  <system name="RGSC">
    <title>Rockstar Games Social Club</title>
    <version os="Windows">
      <locations>
        <path environment_variable="localappdata" path="Rockstar Games\RGSC"/>
      </locations>
      <files type="Saves">
        <save/>
      </files>
      <contributor>AvvA</contributor>
    </version>
  </system>
  <system name="Tomboy">
    <title>Tomboy</title>
    <version>
      <locations>
        <path environment_variable="appdata" path="Tomboy"/>
      </locations>
      <files type="Saves">
        <save path="notes"/>
      </files>
      <files type="Settings">
        <save path="config"/>
      </files>
      <contributor>SanMadJack</contributor>
    </version>
  </system>
  <system name="UPlayData">
    <title>UPlay Data</title>
    <version os="Windows">
      <locations>
        <path environment_variable="installlocation" path="Ubisoft\Ubisoft Game Launcher"/>
        <registry root="local_machine" key="SOFTWARE\UbiSoft\Launcher" value="InstallDir"/>
      </locations>
      <files type="Saves">
        <save path="storage">
          <except path="storage\temp"/>
        </save>
      </files>
      <contributor>Arc Angel</contributor>
    </version>
  </system>
  <system name="XfireData">
    <title>Xfire Data</title>
    <version os="Windows">
      <locations>
        <path environment_variable="allusersprofile" path="Xfire"/>
      </locations>
      <files type="Saves">
        <save/>
      </files>
      <contributor>Arc Angel</contributor>
    </version>
  </system>
</programs>
